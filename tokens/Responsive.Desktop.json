      "com.figma": {
        "hiddenFromPublishing": false,
        "scopes": [
          "ALL_SCOPES"
        ],
        "codeSyntax": {
          "WEB": "var(--sds-responsive-device)"
        }
      }
    }
  },
  "Device Width": {
